    "source": [
      "import numpy as np\n",
      "# The decoder target sequence must be shifted one step to the left relative to the input.\n",
      "# 1. Shift: Removes the <start> token, resulting in a sequence of length 99 (from 100).\n",
      "decoder_target_shifted = train_val[:, 1:]\n",
      "\n",
      "# 2. Pad: Appends a column of zeros to restore the sequence length to 100\n",
      "# to match the model's output tensor shape.\n",
      "decoder_target_data = np.hstack([decoder_target_shifted, np.zeros((train_val.shape[0], 1), dtype=train_val.dtype)])\n",
      "\n",
      "# 3. Expand Dimension: Adds a final dimension of 1, required by Keras's \n",
      "# sparse_categorical_crossentropy loss function for a sequence output.\n",
      "output = np.expand_dims(decoder_target_data, axis=-1)\n",
      "\n",
      "print(f\"New output target shape: {output.shape}\")"
    ]
